!!!
%html
  %head
    %meta{charset: 'utf-8'}
    %meta{name: :viewport, content: 'initial-scale=1.0,width=device-width'}
    = favicon_link_tag
    = favicon_link_tag 'favicon.png', rel: 'icon apple-touch-icon'
    = stylesheet_link_tag    "application", "http://fonts.googleapis.com/css?family=Roboto+Condensed:300,700"

    = csrf_meta_tags
    %title #itscreate

  %body
    = yield
    = javascript_include_tag "application", '//maps.google.com/maps/api/js?v=3.13&sensor=false&libraries=geometry', '//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js', 'http://underscorejs.org/underscore-min.js'
    = content_for(:javascript)

